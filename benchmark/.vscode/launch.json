	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "DEBUG",
			"cargo": {
				"args": [
					"build",
					"--bin=hab-benchmark",
					"--package=hab-benchmark"
				],
				"filter": {
					"name": "hab-benchmark",
					"kind": "bin"
				}
			},
			"args": ["reauth"],
			"cwd": "${workspaceFolder}"
		}
	]
}
